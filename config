apiVersion: v1
kind: Config

clusters:
- cluster:
    certificate-authority-data: ${bamboo_cluster_certificate_authority_data}
    server: ${bamboo_cluster_server}
  name: ${bamboo_cluster_name}

contexts:
- context:
    cluster: ${bamboo_cluster_name}
    user: ${bamboo_cluster_name}
  name: ${bamboo_cluster_name}

current-context: ${bamboo_cluster_name}

users:
- name: ${bamboo_cluster_name}
  user:
    client-certificate-data: ${bamboo_user_client_certificate_data}
    client-key-data: ${bamboo_user_client_key_data}
